{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "google",
    "plugin:prettier/recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 12
  },
  "plugins": [
      "@typescript-eslint"
  ],
  "ignorePatterns": "dist/**/*.js",
  "rules": {
    "valid-jsdoc": "off",
    "linebreak-style": "off",
    "require-jsdoc": "off",
    "max-len": 0,
    "prettier/prettier": ["error", {
        "endOfLine": "auto"
      }
    ],
    "indent": "off",
    "@typescript-eslint/indent": "off"
  }
}
